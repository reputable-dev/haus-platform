{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "bun run build:web",
  "outputDirectory": "dist",
  "framework": "create-react-app",
  "builds": [
    {
      "src": "backend/hono.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/hono.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}